<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navGraphAccount"
    tools:ignore="UnusedNavigation">

    <navigation
        android:id="@+id/navigationTwitter"
        app:startDestination="@id/twitterSignInFragment">

        <fragment
            android:id="@+id/twitterSignInFragment"
            android:name="com.droibit.looking2.account.ui.signin.twitter.TwitterSignInFragment"
            tools:layout="@layout/fragment_twitter_signin">
            <action
                android:id="@+id/toConfirmTwitterSignIn"
                app:destination="@id/twitterSignInConfirmationDialogFragment" />
        </fragment>
        <dialog
            android:id="@+id/twitterSignInConfirmationDialogFragment"
            android:name="com.droibit.looking2.account.ui.signin.twitter.TwitterSignInConfirmationDialogFragment" />

    </navigation>

</navigation>